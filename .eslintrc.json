{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": 0,
        "linebreak-style": 0,
        "react/jsx-one-expression-per-line": 0,
        "jsx-a11y/label-has-for": 0,
        "import/no-unresolved": 0,
        "no-undef": 0,
        "arrow-body-style": 0,
    },
    "plugins": [
        "react"
    ]
}